<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.domidomi.domidomimvp.Controllers.DeliveryStatusActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_gravity="center"
        android:gravity="center"
        android:textSize="18sp"
        android:textColor="#000000"
        android:layout_height="150dp"
        android:text="Su pedido ha sido enviado, en 19 minutos lo tendras!"/>
    <com.timqi.sectorprogressview.SectorProgressView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_height="250dp"
        android:layout_width="250dp"
        android:id="@+id/spv"
        app:bgColor="#e5e5e5"
        android:layout_gravity="center"
        app:fgColor="@color/green"
        app:percent="25"
        app:startAngle="0"
        />
</LinearLayout>
